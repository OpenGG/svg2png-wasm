{
  "public": "..",
  "headers": [
    {
      "source": "**/*.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/example/index.html"
    },
    {
      "source": "/Roboto-Thin.ttf",
      "destination": "/example/Roboto-Thin.ttf"
    }
  ]
}
