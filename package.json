{
  "name": "svg2png-wasm",
  "version": "0.1.7",
  "description": "A svg to png converter made with wasm.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:umd": "wasm-pack build --target web --out-dir pkg-umd",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg-node",
    "postbuild": "rollup -c && run-p copy-wasm:*",
    "clean": "rimraf dist pkg-umd pkg-node umd cjs",
    "demo": "sirv --dev example",
    "copy-wasm:cjs": "copyfiles -f pkg-node/svg2png_wasm_bg.wasm cjs",
    "copy-wasm:esm": "copyfiles -f pkg-node/svg2png_wasm_bg.wasm esm",
    "copy-wasm:umd": "copyfiles -f pkg-umd/svg2png_wasm_bg.wasm umd",
    "prepack": "run-s clean build postbuild",
    "format": "run-p format:*",
    "format:all": "prettier --write .",
    "format:rust": "cargo fmt",
    "lint": "run-p lint:*",
    "lint:js": "prettier --check .",
    "lint:rust": "cargo fmt -- --check && cargo clippy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/svg2png-wasm.git"
  },
  "keywords": [
    "wasm",
    "svg",
    "png"
  ],
  "files": [
    "umd",
    "cjs",
    "esm"
  ],
  "author": "ssssota",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/ssssota/svg2png-wasm/issues"
  },
  "homepage": "https://github.com/ssssota/svg2png-wasm#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.14",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "wasm-pack": "^0.10.1"
  }
}
