{
  "name": "svg2png-wasm",
  "version": "0.5.0",
  "description": "A svg to png converter made with wasm.",
  "main": "main/index.cjs",
  "module": "main/index.js",
  "unpkg": "unpkg/index.js",
  "exports": {
    ".": {
      "import": "./main/index.js",
      "require": "./main/index.cjs",
      "node": "./main/index.js",
      "default": "./main/index.js"
    },
    "./core": {
      "import": "./core/index.js",
      "require": "./core/index.cjs",
      "node": "./core/index.js",
      "default": "./core/index.js"
    }
  },
  "scripts": {
    "make": "run-s build bundle bundle-dts copy-wasm",
    "build": "wasm-pack build --target web --out-dir pkg",
    "bundle": "rollup -c",
    "bundle-dts": "run-p bundle-dts:*",
    "clean": "run-p clean:*",
    "clean:wasm": "rimraf pkg",
    "clean:bundle": "rimraf core main unpkg umd",
    "copy-wasm": "copyfiles -f pkg/svg2png_wasm_bg.wasm .",
    "prepack": "run-s clean make",
    "format": "run-p format:*",
    "format:all": "prettier --write .",
    "format:rust": "cargo fmt",
    "lint": "run-p lint:*",
    "lint:js": "prettier --check .",
    "lint:rust": "cargo fmt -- --check && cargo clippy",
    "test": "run-p test:vrt test:node",
    "test:browser": "playwright test --browser=all --config=test/browser/playwright.config.ts",
    "test:node": "jest -c test/node/jest.config.cjs",
    "test:vrt": "run-s vrt:gen vrt:comp",
    "vrt:gen": "node test/vrt/index.mjs",
    "vrt:comp": "reg-cli test/vrt/actual test/vrt/expected test/vrt/diff",
    "vrt:clean": "rimraf test/vrt/diff test/vrt/actual",
    "update-snapshot": "run-p update-snapshot:*",
    "update-snapshot:vrt": "run-s vrt:clean vrt:gen && reg-cli test/vrt/actual test/vrt/expected test/vrt/diff -U",
    "update-snapshot:browser": "playwright test --browser=all --config=test/browser/playwright.config.ts --update-snapshots",
    "playwright-install": "playwright install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/svg2png-wasm.git"
  },
  "keywords": [
    "wasm",
    "svg",
    "png"
  ],
  "files": [
    "main",
    "svg2png_wasm_bg.wasm",
    "core",
    "umd",
    "unpkg"
  ],
  "author": "ssssota",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/ssssota/svg2png-wasm/issues"
  },
  "homepage": "https://github.com/ssssota/svg2png-wasm#readme",
  "devDependencies": {
    "@playwright/test": "^1.14.1",
    "@swc/core": "^1.2.87",
    "@swc/jest": "^0.2.3",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^5.9.0",
    "esbuild": "^0.13.3",
    "glob": "^7.1.7",
    "jest": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.14.1",
    "prettier": "^2.4.0",
    "reg-cli": "^0.17.3",
    "rimraf": "^3.0.2",
    "serve": "^12.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "wasm-pack": "^0.10.1"
  },
  "type": "module"
}
