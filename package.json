{
  "name": "svg2png-wasm",
  "version": "0.1.1",
  "description": "A svg to png converter made with wasm.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:umd": "webpack",
    "build:node": "wasm-pack build --target nodejs && rollup -c && run-p copy-wasm:*",
    "clean": "rimraf dist pkg umd cjs",
    "demo": "sirv --dev example",
    "copy-wasm:cjs": "copyfiles -f pkg/svg2png_wasm_bg.wasm cjs",
    "copy-wasm:esm": "copyfiles -f pkg/svg2png_wasm_bg.wasm esm",
    "prepare": "run-s clean build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssssota/svg2png-wasm.git"
  },
  "keywords": [
    "wasm",
    "svg",
    "png"
  ],
  "files": [
    "umd",
    "cjs",
    "esm"
  ],
  "author": "ssssota",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/ssssota/svg2png-wasm/issues"
  },
  "homepage": "https://github.com/ssssota/svg2png-wasm#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@wasm-tool/wasm-pack-plugin": "^1.5.0",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.14",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
